package com.neusoft.entity;

public class Rule {
/*	-- 规格表
	create table rule(
	id      int      primary key auto_increment,
	pid     int,-- 商品id,引用商品表的id
	size   varchar(20)   -- 尺寸
	);*/
	
	private int id;
	private int pid;
	private String size;
	public Rule() {
		
	}
	public Rule(int id, int pid, String size) {
		super();
		this.id = id;
		this.pid = pid;
		this.size = size;
	}
	public Rule(int pid, String size) {
		super();
		this.pid = pid;
		this.size = size;
	}

}
